//Switch Example

string dayOfWeek = "Wednesday";
            switch (dayOfWeek)
            [
                case "Wednesday":
                    Console.Out.WriteLine("Discussion Post is Due!");
                    break;
                case "Friday":
                    Console.Out.WriteLine("Quiz is due!");
                    break
                case "Sunday":
                    Console.Out.WriteLine("Assignment is due!");
                    break;
                default:
                    Console.Out.WriteLine("Go Over unit study material");
                    break:
}
